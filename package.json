{
  "name": "cryos",
  "version": "0.1.0",
  "description": "Declarative Web Components in 1Kb",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "run-p dev:*",
    "dev:build": "tsc -w --preserveWatchOutput",
    "dev:build-vite": "vite build --watch",
    "dev:test": "vitest watch",
    "dev:web": "vite --port 8080",
    "test": "vitest run",
    "clean": "rm -rf lib",
    "deploy": "npm run clean && npm run build && npm publish --access public",
    "test:debug": "node --inspect-brk ./node_modules/vitest/vitest.mjs run --no-browser"
  },
  "exports": {
    "./*": "./lib/*.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@vitest/browser": "^2.1.8",
    "@vitest/runner": "^2.1.8",
    "@webgpu/types": "^0.1.35",
    "happy-dom": "^15.11.7",
    "lit": "^2.8.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.49.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.2.2",
    "vite": "^4.3.9",
    "vite-plugin-string": "^1.2.1",
    "vitest": "^2.1.8",
    "webdriverio": "^9.4.1"
  },
  "author": "krisnye@gmail.com",
  "license": "MIT",
  "dependencies": {
  }
}
